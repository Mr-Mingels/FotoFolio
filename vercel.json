{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*).js",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/$1.js"
      },
      {
        "src": "/(.*).css",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)",
        "dest": "server/server.js"
      }
    ]
}
  