{
    "version": 2,
    "builds": [
      {
        "src": "src/server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/server/server.js"
      },
      {
        "src": "/([^\\.]+)$",
        "dest": "src/server/server.js"
      }
    ]
}
  
  