{
    "version": 2,
    "builds": [
      {
        "src": "src/server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/server/server.js"
      },
      {
        "src": "/_next/static/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/_next/static/$1"
      },
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/build/index.html"
      }
    ]
  }
  
  
  