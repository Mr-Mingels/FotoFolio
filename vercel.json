{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/server/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
      "PIXABAY_API_KEY": "@pixabay_api_key"
    }
}
  