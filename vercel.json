{
  "version": 2,
  "builds": [
    {
      "src": "src/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/server.js"
    },
    {
      "src": "/(.*).js",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.js"
    },
    {
      "src": "/(.*).css",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


  
  